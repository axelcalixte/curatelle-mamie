{
  "name": "maman",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write \"src/**/*.{html,ts,css,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{html,ts,css,scss,json,md}\""
  },
  "prettier": {
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.1.3",
    "daisyui": "^5.5.18",
    "papaparse": "^5.5.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/papaparse": "^5.5.2",
    "@types/wicg-file-system-access": "^2023.10.7",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.18"
  }
}
